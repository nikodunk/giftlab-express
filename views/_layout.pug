    // Footer
    footer.footer.bg-light.center(style="")

        //- Email signup
        section(id="signup").padding.center
            //- .overlay
            .container
              .row.signupform
                .col-xl-9.mx-auto
                  h4.mb-4 STAY UPDATED
                  p.lead Connect with us for the latest updates, including new non profit listings and Gift Lab feature updates.

                .col-md-10.col-lg-8.col-xl-7.mx-auto
                  form(id="asdf" action="/signup" method="post")
                    div(class="input-group mb-3")
                      input(class="form-control" type='email' placeholder='Enter your email' name='email' id="email")
                      div(class="input-group-append")
                        input(type="submit" id="register" value="SUBSCRIBE" style="background-color: #00AFE1 !important;").btn.btn-primary.titleButton


        script.
            $("#asdf").submit(function(e) {
                var form = $(this);
                var url = form.attr('action');
                console.log(url, $('#email').val())
                $.ajax({
                       type: "POST",
                       url: url,
                       data: {email: $('#email').val()}, // serializes the form's elements.
                       success: function(data)
                       {
                           $( ".signupform" ).replaceWith( "<br><br><h3 style='color: white'>Successfully signed up!</h3><br><br><br>" )
                        
                       }
                     });

                e.preventDefault(); // avoid to execute the actual submit of the form.
            });

                        

